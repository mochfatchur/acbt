{
  "name": "acbt-fe",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently -k \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "electron .",
    "dev:renderer": "vite --host",
    "build": "vite build && electron-builder",
    "start": "electron ."
  },
  "build": {
    "appId": "com.acbt.collector",
    "productName": "ACBT Commit Collector",
    "directories": {
      "buildResources": "build",
      "output": "dist-electron"
    },
    "files": [
      "dist/**",
      "main.js",
      "preload.js",
      "package.json"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "electron": "^39.2.1",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "vite": "^7.2.2"
  },
  "dependencies": {
    "electron-store": "^11.0.2",
    "vue": "^3.5.24"
  }
}
